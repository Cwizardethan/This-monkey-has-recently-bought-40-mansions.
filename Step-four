#include <bits/stdc++.h>
using namespace std;
#define int long long
const int inf = 0x3f3f3f3f3f3f3f3f;
int n, t;
void solve()
{
    cin >> n;
    priority_queue<int, vector<int>, greater<int>> pq;
    vector<pair<int, int>> balls; // wei shen me
    for (int i = 0; i < n; i++)
    {
        int a, b;
        cin >> a >> b;
        balls.push_back({a, b});
    }
    balls.push_back({inf, inf});
    sort(balls.begin(), balls.end());
    int i = 0;
    while(i < n){
        pq.push(balls[i].second);
        int left = balls[i].first, j = i+1;
        while(balls[j].first == left){
            pq.push(balls[j].second);
            j++;
        }
        while(pq.size() && left < balls[j].first){
            int end = pq.top();
            pq.pop();
            if(end < left){
                cout<<"NO"<<endl;
                return;
            }
            left++;
        }
        i = j;

    }
    cout<<"YES"<<endl;

}
signed main()
{
    cin >> t;
    while (t--)
    {
        solve();
    }
}